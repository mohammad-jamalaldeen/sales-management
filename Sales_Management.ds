				 float newOrder.calcGrandTotal(float subTotal, float tax, float discount)
                 {
                 	subTotalVal = ifnull(subTotal,0.00);
                 	taxVal = ifnull(tax,0.00);
                 	discountVal = ifnull(discount,0.00);
                 	getDiscount = (subTotalVal * discountVal) / 100;
                 	getTax = ((subTotalVal - getDiscount) * taxVal) / 100;
                 	returnTotal = subTotalVal - getDiscount + getTax;
                 	return returnTotal;
                 }
